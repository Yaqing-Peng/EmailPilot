{
    "manifest_version": 3,
    "name": "Gmail Extension Combine",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://mail.google.com/*",
                "https://outlook.office365.com/*"
            ],
            "js": ["dist/bundle.js"],
            "run_at": "document_end"
        }
    ]  
}